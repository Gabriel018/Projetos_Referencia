@{
	ViewData["Title"] = "Home Page";
}


<div class="Container_menu_Admin">


	 <div class="TituloAdm">
	 <h1>Menu de Administraçao geral</h1>
	  </div>

	<a class="text-secondary-emphasis fs-5" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Bem Vindo @User.Identity?.Name!</a>


   <div class="Menu_Adm" >
  <div class="btn-group" role="group" aria-label="Basic checkbox toggle button group">
  <input type="checkbox" class="btn-check" id="btncheck1" autocomplete="off">
  <a class="btn btn-outline-dark fs-5" asp-controller="Relatorios" asp-action="Index" for="btncheck1">Relatorios</a>

  <input type="checkbox" class="btn-check" id="btncheck2" autocomplete="off">
  <a class="btn btn-outline-dark fs-5"   asp-controller="Clientes" asp-action="Index" for="btncheck2">Clientes</a>

  <input type="checkbox" class="btn-check" id="btncheck3" autocomplete="off">
  <a class="btn btn-outline-dark fs-5" asp-controller="Vendedors" asp-action="Index" for="btncheck3">Vendedores</a>

  <input type="checkbox" class="btn-check" id="btncheck3" autocomplete="off">
  <a class="btn btn-outline-dark fs-5" asp-controller="Adms" asp-action="Index" for="btncheck3">Administradores</a>
  <input type="checkbox" class="btn-check" id="btncheck3" autocomplete="off">
</div>

</div

</div>

