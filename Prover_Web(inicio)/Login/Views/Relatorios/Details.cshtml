@model Prover_Web.Models.Vendedor

@{
    ViewData["Title"] = "Details";
}

<div>
    <h4>Detalhes</h4>
     <div >

        <div class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Nome)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Nome)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Cpf)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Cpf)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Telefone)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Telefone)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Descricao)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Descricao)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Categoria)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Categoria)
            </dd>
        </div>



     </div>
    
    
    <a class="btn btn-dark" asp-controller="Relatorios" asp-action="PesquisarPorMath">Voltar</a>
    <button type="button" class="btn btn-success" id="notific" onclick="notificar()">Notificar vendedor</button>
</div>

<div id="myModal" class="modal">

  <!-- Modal content -->
  <div class="modal-content">
    <span class="close">&times;</span>
    <h2 class="font-modal-titulo" >Personal Style Notificado</h2>
    <p class="font-modal">Ele recebera sua notificaçao e entrara em contato parabens!!!</p>
  </div>

</div>

