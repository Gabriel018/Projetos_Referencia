@model IEnumerable<Prover_Web.Models.ClienteVendedor>

<h1>Lista de Clientes e Vendedores</h1>

<table class="table">
    <thead>
        <tr>
            <th>Cliente</th>
            <th>Categoria do Cliente</th>
            <th>Nome do Vendedor</th>
            <th>Categoria do Vendedor</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Cliente.Nome</td>
                <td>@item.Vendedor.Nome</td>
                <td>@item.Vendedor.Categoria</td>
            </tr>
        }
    </tbody>
</table>
